{
    "name": "manticora/sport-centre",
    "description": "Sport Centre Domain Model",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Giustiniani",
            "email": "mariogiustiniani@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
       "php":                            ">=5.3.2",
       "psr/log":                        "~1.0",
       "friendsofsymfony/rest-bundle":   "~1.4",
        "nelmio/api-doc-bundle": "@stable",
        "jms/serializer-bundle": "dev-master",
        "willdurand/hateoas-bundle": "@stable",
        "cilex/cilex": "1.0.*@dev"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "Manticora\\SportCentre\\": "src/",
            "Manticora\\SportCentre\\Infrastucture\\Symfony\\Bundle\\ApiBundle\\Features\\Context\\":"features/bootstrap"
        }
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "3.7.*@stable",
        "behat/symfony2-extension":     "~2.0",
        "behat/mink-extension":         "~2.0",
        "behat/mink-browserkit-driver": "~1.2",
        "symfony/monolog-bundle":       "~2.3",
        "symfony/browser-kit":       "~2.3",
        "symfony/form":       "~2.3",
        "symfony/validator":       "~2.3",
        "sensio/framework-extra-bundle":       "~2.3"
    }
}
